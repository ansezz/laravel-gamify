{
    "name": "ansezz/laravel-gamify",
    "description": "Add gamification in laravel app with reputation point and badges support",
    "homepage": "https://github.com/ansezz/laravel-gamify",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ansezz",
        "laravel",
        "achivement",
        "gamify",
        "gamification",
        "reputation",
        "points",
        "badge",
        "reward"
    ],
    "authors": [
        {
            "name": "Anass Ez-zouaine",
            "email": "ansezzouaine@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "~3.8|^4.0",
        "mockery/mockery": "^0.9.4 || ~1.0"
    },
    "autoload": {
        "psr-4": {
            "Ansezz\\Gamify\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ansezz\\Gamify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ansezz\\Gamify\\GamifyServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
